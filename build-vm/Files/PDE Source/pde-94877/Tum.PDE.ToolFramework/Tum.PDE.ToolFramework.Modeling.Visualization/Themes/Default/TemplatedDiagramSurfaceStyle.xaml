                    xmlns:vm="clr-namespace:Tum.PDE.ToolFramework.Modeling.Visualization.ViewModel.DiagramSurface.Templates"
                    xmlns:ctrl="clr-namespace:Tum.PDE.ToolFramework.Modeling.Visualization.Controls.DiagramSurface.Templates">
    <DataTemplate DataType="{x:Type vm:ModalDesignerSurfaceViewModel}">
        <ctrl:ModalDiagram HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SpecificElementsDesignerSurfaceViewModel}">
